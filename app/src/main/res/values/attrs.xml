    <declare-styleable name="GradientIndicator">
        <attr name="leftColor" format="color"/>
        <attr name="rightColor" format="color"/>
        <attr name="minVal" format="string"/>
        <attr name="maxVal" format="string"/>
        <attr name="currentVal" format="string"/>
    </declare-styleable>
</resources>